{
  "name": "eessi-pensjon-saksbehandling-ui",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "start": "cross-env NODE_ENV=development npm-run-all -p start:*",
    "start:css": "less-watch-compiler --config less.json",
    "start:js": "react-scripts start",
    "build": "npm run build:css && npm run build:js && npm run build:git",
    "build:git": "export GIT_VERSION=`git describe --always` && sed -i -- \"s/%GIT_VERSION%/$GIT_VERSION/g\" build/index.html",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "build:js": "cross-env NODE_OPTIONS=--max-old-space-size=4096 react-scripts build",
    "test": "cross-env NODE_ENV=test react-scripts test",
    "test:ci": "cross-env NODE_ENV=test react-scripts test --updateSnapshot --watchAll=false",
    "test:coverage": "cross-env NODE_ENV=test react-scripts test --coverage  --watchAll=false",
    "start-json": "json-server --watch db.json --routes routes.json --port 8080",
    "cucumber": "cucumber-js src/tests/**/*.feature -f node_modules/cucumber-pretty",
    "wcag-test": "nav-ally -f wcag.yml"
  },
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "browser-cookies": "^1.2.0",
    "classnames": "^2.2.6",
    "core-js": "^3.2.1",
    "cross-fetch": "^3.0.4",
    "eessi-pensjon-ui": "https://github.com/navikt/eessi-pensjon-ui#1",
    "history": "^4.10.1",
    "http-proxy-middleware": "^0.20.0",
    "i18next": "^17.0.16",
    "i18next-browser-languagedetector": "^3.0.3",
    "i18next-xhr-backend": "^3.2.0",
    "lodash": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "rc-collapse": "^1.11.7",
    "rc-slider": "^8.7.1",
    "react": "^16.10.1",
    "react-beautiful-dnd": "^11.0.5",
    "react-dnd": "^9.4.0",
    "react-dnd-html5-backend": "^9.4.0",
    "react-dom": "^16.10.1",
    "react-grid-layout": "https://github.com/navikt/react-grid-layout#1.0.1",
    "react-i18next": "^10.13.1",
    "react-pdf": "^4.1.0",
    "react-resize-detector": "^4.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "reinspect": "^1.0.2",
    "sprintf-js": "^1.1.2",
    "url-search-params-polyfill": "^7.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.6.2",
    "acorn": "^7.1.0",
    "cross-env": "^6.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "es6-promise": "^4.2.8",
    "identity-obj-proxy": "^3.0.0",
    "jest-fetch-mock": "^2.1.2",
    "json-server": "^0.15.1",
    "less": "^3.10.3",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.14.1",
    "mock-socket": "^9.0.0",
    "nock": "^11.3.5",
    "npm-run-all": "^4.1.5",
    "react-scripts": "^3.1.2",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "standard": "^14.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "standard": {
    "ignore": [
      "public/*.js"
    ],
    "parser": "babel-eslint",
    "globals": [
      "Blob",
      "File",
      "XMLHttpRequest",
      "HTMLCanvasElement",
      "FileReader",
      "ReactDOM",
      "WebSocket",
      "act",
      "fetch",
      "describe",
      "it",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "shallow",
      "mount",
      "render",
      "renderer",
      "jest"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      },
      "src/actions": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverageFrom": [
      "src/actions/*.js",
      "src/applications/*.js",
      "src/applications/**/*.js",
      "src/components/*.js",
      "src/components/**/*.js",
      "src/pages/*.js",
      "src/pages/**/*.js",
      "src/reducers/*.js",
      "src/utils/*.js",
      "src/widgets/*.js",
      "src/widgets/**/*.js"
    ]
  }
}
