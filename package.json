    "nav-frontend-paneler-style": "^0.3.21",
    "nav-frontend-popover": "0.0.27",
    "nav-frontend-popover-style": "0.0.5",
    "nav-frontend-skjema": "^3.0.13",
    "nav-frontend-skjema-style": "^2.0.9",
    "nav-frontend-spinner": "^2.0.4",
    "nav-frontend-spinner-style": "^0.2.5",
    "nav-frontend-tabell-style": "^0.2.23",
    "nav-frontend-tabs": "0.0.43",
    "nav-frontend-tabs-style": "0.0.27",
