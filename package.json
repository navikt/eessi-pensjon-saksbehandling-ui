{
  "name": "eessi-pensjon-saksbehandling-ui",
  "version": "1.1.0",
  "license": "MIT",
  "description": "EESSI Pensjon Saksbehandling UI",
  "scripts": {
    "npx": "npx npm-force-resolutions",
    "build": "npm run build:css && npm run build:js && npm run build:git",
    "build:git": "export GIT_VERSION=`git describe --always` && sed -i -- \"s/%GIT_VERSION%/$GIT_VERSION/g\" build/index.html",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "build:js": "cross-env NODE_OPTIONS=--max-old-space-size=8192 react-scripts build",
    "lint": "standardx src/*.ts* src/*/*.ts* src/*/*/*.ts* src/*/*/*/*.ts* src/*/*/*/*/*.ts*",
    "lint:fix": "standardx src/*.ts* src/*/*.ts* src/*/*/*.ts* src/*/*/*/*.ts* src/*/*/*/*/*.ts* --fix",
    "start": "cross-env NODE_ENV=development npm-run-all -p start:*",
    "start:css": "less-watch-compiler --config less.json",
    "start:js": "cross-env NODE_OPTIONS=--max-old-space-size=4096 react-scripts start",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test react-scripts test",
    "test:clear": "cross-env NODE_ENV=test react-scripts test --clearCache",
    "test:ci": "cross-env NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test react-scripts test --watchAll=false --forceExit --updateSnapshot",
    "test:coverage": "cross-env NODE_ENV=test react-scripts test --coverage  --watchAll=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/eessi-pensjon-saksbehandling-ui.git"
  },
  "private": true,
  "dependencies": {
    "@sentry/browser": "^5.19.2",
    "amplitude-js": "^7.1.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "filopplasting": "^1.0.1",
    "flagg-ikoner": "^1.0.3",
    "forhandsvisningsfil": "^1.0.4",
    "fremdriftslinje": "^1.0.1",
    "history": "^4.10.1",
    "http-proxy-middleware": "^1.0.5",
    "i18next": "^19.6.0",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-xhr-backend": "^3.2.2",
    "js-fetch-api": "^1.0.0",
    "land-verktoy": "^1.0.1",
    "landvelger": "^1.0.2",
    "lodash": "^4.17.19",
    "lodash.throttle": "^4.1.1",
    "md5": "^2.2.1",
    "moment": "^2.27.0",
    "nav-dashboard": "1.0.4",
    "nav-frontend-alertstriper": "^3.0.20",
    "nav-frontend-alertstriper-style": "^2.0.17",
    "nav-frontend-chevron": "^1.0.12",
    "nav-frontend-chevron-style": "^0.3.5",
    "nav-frontend-core": "^5.0.1",
    "nav-frontend-ekspanderbartpanel": "^3.0.21",
    "nav-frontend-ekspanderbartpanel-style": "^1.0.19",
    "nav-frontend-etiketter": "^2.0.5",
    "nav-frontend-etiketter-style": "^1.0.3",
    "nav-frontend-hjelpetekst": "^2.0.31",
