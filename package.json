{
  "name": "eessi-pensjon-saksbehandling-ui",
  "version": "1.1.0",
  "license": "MIT",
  "description": "EESSI Pensjon Saksbehandling UI",
  "scripts": {
    "build": "npm run build:css && npm run build:js && npm run build:git",
    "build:git": "export GIT_VERSION=`git describe --always` && sed -i -- \"s/%GIT_VERSION%/$GIT_VERSION/g\" build/index.html",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "build:js": "cross-env NODE_OPTIONS=--max-old-space-size=8192 react-scripts build",
    "lint": "standardx src/*.ts* src/*/*.ts* src/*/*/*.ts* src/*/*/*/*.ts* src/*/*/*/*/*.ts*",
    "lint:fix": "standardx src/*.ts* src/*/*.ts* src/*/*/*.ts* src/*/*/*/*.ts* src/*/*/*/*/*.ts* --fix",
    "start": "cross-env NODE_ENV=development npm-run-all -p start:*",
    "start:css": "less-watch-compiler --config less.json",
    "start:js": "cross-env NODE_OPTIONS=--max-old-space-size=4096 react-scripts start",
    "test": "cross-env NODE_ENV=test react-scripts test",
    "test:ci": "cross-env NODE_ENV=test react-scripts test --updateSnapshot --watchAll=false",
    "test:coverage": "cross-env NODE_ENV=test react-scripts test --coverage  --watchAll=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/eessi-pensjon-saksbehandling-ui.git"
  },
  "private": true,
  "dependencies": {
    "@types/react-dom": "^16.9.5",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "eessi-pensjon-ui": "https://github.com/navikt/eessi-pensjon-ui#2.0.8",
    "history": "^4.10.1",
    "http-proxy-middleware": "^0.20.0",
    "i18next": "^19.1.0",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-thunk-reducer": "^0.2.4",
    "react-i18next": "^11.3.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.3",
    "react-resize-detector": "^4.2.1",
    "react-router": "^5.1.2",
    "react-snowstorm": "0.0.3",
    "react-tooltip": "^3.11.2",
    "redux": "^4.0.5",
    "sprintf-js": "^1.1.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.8.3",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^25.1.0",
    "@types/lodash": "^4.14.149",
    "@types/mustache": "^4.0.0",
    "@types/react": "^16.9.19",
    "@types/react-loadable": "^5.5.3",
    "@types/react-redux": "^7.1.7",
    "@types/react-resize-detector": "^4.2.0",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-tooltip": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "acorn": "^7.1.0",
    "cross-env": "^7.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "es6-promise": "^4.2.8",
    "identity-obj-proxy": "^3.0.0",
    "jest-fetch-mock": "^3.0.1",
    "json-server": "^0.15.1",
    "less": "^3.10.3",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.14.6",
    "mock-socket": "^9.0.3",
    "mustache": "^4.0.0",
    "nock": "^11.7.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-axe": "^3.3.0",
    "react-scripts": "^3.2.0",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "standardx": "^5.0.0",
    "ts-jest": "^25.0.0",
    "typescript": "^3.7.5"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "ignore": [
      "public/**/*.js",
      "node_modules/**/*.js"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "globals": [
      "Blob",
      "File",
      "XMLHttpRequest",
      "HTMLCanvasElement",
      "FileReader",
      "ReactDOM",
      "WebSocket",
      "act",
      "fetch",
      "describe",
      "it",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "shallow",
      "mount",
      "render",
      "renderer",
      "jest"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "src/actions": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "src/actions/*.{ts,tsx}",
      "src/applications/**/*.{ts,tsx}",
      "!src/applications/**/*.d.ts",
      "src/components/*.{ts,tsx}",
      "!src/components/*.d.ts",
      "src/components/**/*.{ts,tsx}",
      "!src/components/**/*.d.ts",
      "src/pages/**/*.{ts,tsx}",
      "!src/pages/**/*.d.ts",
      "src/reducers/*.{ts,tsx}",
      "src/widgets/*.{ts,tsx}",
      "!src/widgets/*.d.ts",
      "src/widgets/**/*.{ts,tsx}",
      "!src/widgets/**/*.d.ts"
    ]
  }
}
