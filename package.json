{
  "name": "eessi-pensjon-saksbehandling-ui",
  "version": "3.0.1",
  "license": "MIT",
  "description": "EESSI Pensjon Saksbehandling UI",
  "scripts": {
    "npx": "npx npm-force-resolutions",
    "postbuild": "export GIT_VERSION=`git describe --always` && sed -i -- \"s/%GIT_VERSION%/$GIT_VERSION/g\" build/index.html",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "start": "NODE_OPTIONS=--max-old-space-size=4096 vite",
    "test": "NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test jest",
    "test:vitest": "NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test vitest",
    "test:filtered": "NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test jest --config jest.filtered.config.cjs",
    "test:clear": "NODE_ENV=test jest test --clearCache",
    "test:ci": "NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test jest test --watchAll=false --forceExit --updateSnapshot",
    "test:coverage": "NODE_ENV=test jest test --coverage  --watchAll=false",
    "preview": "vite preview",
    "knip": "knip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/eessi-pensjon-saksbehandling-ui.git"
  },
