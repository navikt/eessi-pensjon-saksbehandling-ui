{
  "name": "eessi-pensjon-saksbehandling-ui",
  "version": "2.0.0",
  "license": "MIT",
  "description": "EESSI Pensjon Saksbehandling UI",
  "scripts": {
    "npx": "npx npm-force-resolutions",
    "build": "npm run build:css && npm run build:js && npm run build:git",
    "build:git": "export GIT_VERSION=`git describe --always` && sed -i -- \"s/%GIT_VERSION%/$GIT_VERSION/g\" build/index.html",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "build:js": "cross-env NODE_OPTIONS=--max-old-space-size=8192 react-scripts build",
    "lint": "standardx './src/**/*.ts' './src/**/*.tsx'",
    "lint:fix": "standardx './src/**/*.ts' './src/**/*.tsx' --fix",
    "start": "cross-env NODE_ENV=development npm-run-all -p start:*",
    "start:css": "less-watch-compiler --config less.json",
    "start:js": "cross-env NODE_OPTIONS=--max-old-space-size=4096 react-scripts start",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test react-scripts test",
    "test:clear": "cross-env NODE_ENV=test react-scripts test --clearCache",
    "test:ci": "cross-env NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test react-scripts test --watchAll=false --forceExit --updateSnapshot",
    "test:coverage": "cross-env NODE_ENV=test react-scripts test --coverage  --watchAll=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/eessi-pensjon-saksbehandling-ui.git"
  },
  "private": true,
  "dependencies": {
    "@navikt/fnrvalidator": "^1.1.3",
    "@sentry/browser": "^6.2.5",
    "amplitude-js": "^8.1.0",
    "classnames": "^2.3.1",
    "core-js": "^3.10.1",
    "filopplasting": "^1.0.8",
    "flagg-ikoner": "^1.1.1",
    "forhandsvisningsfil": "^1.1.3",
    "fremdriftslinje": "^1.0.6",
    "history": "4.10.1",
    "http-proxy-middleware": "^1.1.2",
    "i18next": "^20.2.1",
    "i18next-browser-languagedetector": "^6.1.0",
    "i18next-xhr-backend": "^3.2.2",
    "js-fetch-api": "^1.0.8",
    "land-verktoy": "^1.0.6",
    "landvelger": "^1.2.0",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "nav-dashboard": "^1.1.7",
    "nav-frontend-alertstriper": "^3.1.1",
    "nav-frontend-alertstriper-style": "^2.1.1",
    "nav-frontend-chevron": "^1.0.28",
    "nav-frontend-chevron-style": "^1.0.4",
    "nav-frontend-core": "^5.1.1",
    "nav-frontend-ekspanderbartpanel": "^3.1.1",
    "nav-frontend-ekspanderbartpanel-style": "^1.1.1",
    "nav-frontend-etiketter": "^2.1.1",
    "nav-frontend-etiketter-style": "^1.1.1",
    "nav-frontend-js-utils": "^1.0.17",
    "nav-frontend-knapper": "^2.1.1",
    "nav-frontend-knapper-style": "^1.1.1",
    "nav-frontend-lenkepanel": "^1.1.1",
    "nav-frontend-lenker": "^1.1.1",
    "nav-frontend-lenker-style": "^1.1.1",
    "nav-frontend-lukknapp": "^1.1.1",
    "nav-frontend-lukknapp-style": "^1.1.1",
    "nav-frontend-modal": "^1.1.2",
    "nav-frontend-modal-style": "^1.1.1",
    "nav-frontend-paneler": "^2.1.1",
    "nav-frontend-paneler-style": "^1.1.1",
    "nav-frontend-skjema": "^3.2.1",
    "nav-frontend-skjema-style": "^2.2.1",
    "nav-frontend-spinner": "^2.0.16",
    "nav-frontend-spinner-style": "^1.0.2",
    "nav-frontend-tabell-style": "^1.1.1",
    "nav-frontend-tabs": "1.1.1",
    "nav-frontend-tabs-style": "1.1.1",
    "nav-frontend-typografi": "^3.1.1",
    "nav-frontend-typografi-style": "^1.1.1",
    "nav-frontend-veileder": "^2.1.2",
    "nav-frontend-veileder-style": "^2.1.1",
    "nav-frontend-veilederpanel": "^1.1.2",
    "nav-frontend-veilederpanel-style": "^1.1.1",
    "nav-hoykontrast": "^1.2.6",
    "print-js": "^1.6.0",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-collapse": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-i18next": "11.8.13",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.13.1",
    "react-redux": "^7.2.3",
    "react-resize-detector": "^6.6.5",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.0",
    "react-to-print": "^2.12.4",
    "redux": "^4.0.5",
    "sprintf-js": "^1.1.2",
    "styled-components": "^5.2.3",
    "tabell": "^1.3.6",
    "use-error-boundary": "^2.0.4"
  },
  "devDependencies": {
    "@axe-core/react": "^4.1.1",
    "@babel/runtime": "^7.13.10",
    "@types/amplitude-js": "^7.0.1",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/md5": "^2.3.0",
    "@types/mustache": "^4.1.1",
    "@types/react": "^17.0.3",
    "@types/react-collapse": "^5.0.0",
    "@types/react-dom": "^17.0.3",
    "@types/react-loadable": "^5.5.5",
    "@types/react-modal": "^3.12.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-resize-detector": "^5.0.0",
    "@types/react-router": "^5.1.13",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.15",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "es6-promise": "^4.2.8",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-styled-components": "^7.0.3",
    "less": "4.1.1",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.14.6",
    "mock-socket": "^9.0.3",
    "mustache": "^4.2.0",
    "nav-frontend-ikoner-assets": "^2.0.10",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-scripts": "^4.0.3",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "standardx": "^7.0.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "node-notifier": "=8.0.1",
    "is-svg": ">=4.2.2",
    "react-dev-utils": ">=11.0.4"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "rules": {
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    },
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "fragment": "Fragment",
        "version": "detect",
        "flowVersion": "0.53"
      },
      "propWrapperFunctions": [
        "forbidExtraProps",
        {
          "property": "freeze",
          "object": "Object"
        },
        {
          "property": "myFavoriteWrapper"
        }
      ],
      "linkComponents": [
        "Hyperlink",
        {
          "name": "Link",
          "linkAttribute": "to"
        }
      ]
    }
  },
  "standardx": {
    "ignore": [
      "public/",
      "*.snap"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "react-hooks"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "globals": [
      "act",
      "afterAll",
      "afterEach",
      "beforeAll",
      "beforeEach",
      "Blob",
      "describe",
      "Element",
      "expect",
      "fetch",
      "File",
      "FileReader",
      "HTMLAnchorElement",
      "HTMLButtonElement",
      "HTMLCanvasElement",
      "HTMLInputElement",
      "HTMLTextAreaElement",
      "HTMLSelectElement",
      "HTMLElement",
      "it",
      "jest",
      "JSX",
      "MessageEvent",
      "mount",
      "React",
      "ReactDOM",
      "render",
      "renderer",
      "shallow",
      "WebSocket",
      "Window",
      "XMLHttpRequest"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "src/actions": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "src/actions/*.{ts,tsx}",
      "src/applications/**/*.{ts,tsx}",
      "src/components/**/*.{ts,tsx}",
      "src/pages/**/*.{ts,tsx}",
      "src/reducers/*.{ts,tsx}",
      "src/widgets/*.{ts,tsx}",
      "src/widgets/**/*.{ts,tsx}"
    ]
  }
}
