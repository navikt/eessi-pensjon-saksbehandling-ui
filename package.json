{
  "name": "eessi-pensjon-saksbehandling-ui",
  "version": "1.1.0",
  "license": "MIT",
  "description": "EESSI Pensjon Saksbehandling UI",
  "scripts": {
    "lint": "standardx **/*.tsx",
    "lint:fix": "standardx **/*.tsx --fix",
    "start": "cross-env NODE_ENV=development npm-run-all -p start:*",
    "start:css": "less-watch-compiler --config less.json",
    "start:js": "cross-env NODE_OPTIONS=--max-old-space-size=4096 react-scripts start",
    "build": "npm run build:css && npm run build:js && npm run build:git",
    "build:git": "export GIT_VERSION=`git describe --always` && sed -i -- \"s/%GIT_VERSION%/$GIT_VERSION/g\" build/index.html",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "build:js": "cross-env NODE_OPTIONS=--max-old-space-size=8192 react-scripts build",
    "test": "cross-env NODE_ENV=test react-scripts test",
    "test:ci": "cross-env NODE_ENV=test react-scripts test --updateSnapshot --watchAll=false",
    "test:coverage": "cross-env NODE_ENV=test react-scripts test --coverage  --watchAll=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/eessi-pensjon-saksbehandling-ui.git"
  },
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.4.5",
    "eessi-pensjon-ui": "https://github.com/navikt/eessi-pensjon-ui#1.4.3",
    "history": "^4.10.1",
    "http-proxy-middleware": "^0.20.0",
    "i18next": "^19.0.1",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-thunk-reducer": "^0.2.1",
    "react-i18next": "^11.2.4",
    "react-loadable": "^5.5.0",
    "react-router": "^5.1.2",
    "react-snowstorm": "0.0.3",
    "react-tooltip": "^3.11.1",
    "sprintf-js": "^1.1.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.7.4",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.13",
    "@types/react-loadable": "^5.5.2",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "acorn": "^7.1.0",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "es6-promise": "^4.2.8",
    "identity-obj-proxy": "^3.0.0",
    "jest-fetch-mock": "^2.1.2",
    "json-server": "^0.15.1",
    "less": "^3.10.3",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.14.1",
    "mock-socket": "^9.0.2",
    "mustache": "^3.1.0",
    "nock": "^11.7.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-app-polyfill": "^1.0.4",
    "react-axe": "^3.3.0",
    "react-scripts": "^3.2.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "standardx": "^5.0.0",
    "typescript": "^3.7.2"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "standardx": {
    "ignore": [
      "public/**/*.js",
      "node_modules/**/*.js"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "globals": [
      "Blob",
      "File",
      "XMLHttpRequest",
      "HTMLCanvasElement",
      "FileReader",
      "ReactDOM",
      "WebSocket",
      "act",
      "fetch",
      "describe",
      "it",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "shallow",
      "mount",
      "render",
      "renderer",
      "jest"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      },
      "src/actions": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "src/actions/*.js",
      "src/applications/*.js",
      "src/applications/**/*.js",
      "src/components/*.js",
      "src/components/**/*.js",
      "src/pages/*.js",
      "src/pages/**/*.js",
      "src/reducers/*.js",
      "src/utils/*.js",
      "src/widgets/*.js",
      "src/widgets/**/*.js",
      "src/actions/*.tsx",
      "src/applications/*.tsx",
      "src/applications/**/*.tsx",
      "src/components/*.tsx",
      "src/components/**/*.tsx",
      "src/pages/*.tsx",
      "src/pages/**/*.tsx",
      "src/reducers/*.tsx",
      "src/utils/*.tsx",
      "src/widgets/*.tsx",
      "src/widgets/**/*.tsx"
    ]
  }
}
