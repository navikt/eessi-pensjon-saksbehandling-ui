    "react-app-polyfill": "^1.0.6",
    "react-collapse": "^5.0.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.0",
    "react-resize-detector": "^5.0.6",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "react-to-print": "^2.9.0",
    "redux": "^4.0.5",
    "sprintf-js": "^1.1.2",
    "styled-components": "^5.1.1",
    "tabell": "^1.0.4",
    "use-error-boundary": "^1.2.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.10.4",
    "@types/amplitude-js": "^5.11.0",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.157",
    "@types/md5": "^2.2.0",
    "@types/mustache": "^4.0.1",
    "@types/react": "^16.9.43",
    "@types/react-collapse": "^5.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-loadable": "^5.5.3",
    "@types/react-redux": "^7.1.9",
    "@types/react-resize-detector": "^4.2.0",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-select": "^3.0.14",
    "@types/styled-components": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "es6-promise": "^4.2.8",
    "identity-obj-proxy": "^3.0.0",
    "less": "^3.12.0",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.14.6",
    "mock-socket": "^9.0.3",
    "mustache": "^4.0.1",
    "nav-styled-component-theme": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-scripts": "^3.4.1",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "standardx": "^5.0.0",
    "ts-jest": "^26.1.2",
    "typescript": "^3.9.6"
  },
  "resolutions": {
    "yargs-parser": "18.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "ignore": [
      "public/**/*.js",
      "node_modules/**/*.js",
      "src/**/*.snap"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "react-hooks"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "globals": [
      "Blob",
      "File",
      "XMLHttpRequest",
      "HTMLCanvasElement",
      "FileReader",
      "ReactDOM",
      "WebSocket",
      "act",
      "fetch",
      "describe",
      "it",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "shallow",
      "mount",
      "render",
      "renderer",
      "jest"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "src/actions": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "src/actions/*.{ts,tsx}",
      "src/applications/**/*.{ts,tsx}",
      "src/components/**/*.{ts,tsx}",
      "src/pages/**/*.{ts,tsx}",
      "src/reducers/*.{ts,tsx}",
      "src/widgets/*.{ts,tsx}",
      "src/widgets/**/*.{ts,tsx}"
    ]
  }
}
