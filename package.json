{
  "name": "eessi-pensjon-saksbehandling-ui",
  "version": "3.0.1",
  "license": "MIT",
  "description": "EESSI Pensjon Saksbehandling UI",
  "scripts": {
    "npx": "npx npm-force-resolutions",
    "postbuild": "export GIT_VERSION=`git describe --always` && sed -i -- \"s/%GIT_VERSION%/$GIT_VERSION/g\" build/index.html",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "lint": "standardx './src/**/*.ts' './src/**/*.tsx'",
    "lint:fix": "standardx './src/**/*.ts' './src/**/*.tsx' --fix",
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test jest",
    "test:vitest": "cross-env NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test vitest",
    "test:filtered": "cross-env NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test jest --config jest.filtered.config.cjs",
    "test:clear": "cross-env NODE_ENV=test jest test --clearCache",
    "test:ci": "cross-env NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=test jest test --watchAll=false --forceExit --updateSnapshot",
    "test:coverage": "cross-env NODE_ENV=test jest test --coverage  --watchAll=false",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/eessi-pensjon-saksbehandling-ui.git"
  },
  "private": true,
  "dependencies": {
    "@navikt/ds-css": "^7.18.0",
    "@navikt/ds-react": "^7.18.0",
    "@navikt/fetch": "^5.0.4",
    "@navikt/flagg-ikoner": "^10.0.1",
    "@navikt/fnrvalidator": "^1.3.0",
    "@navikt/land-verktoy": "^9.1.10",
    "@navikt/landvelger": "^10.0.3",
    "@navikt/oasis": "^3.8.0",
    "@navikt/tabell": "^8.0.4",
    "@reduxjs/toolkit": "^1.8.5",
    "@vitejs/plugin-react": "^4.3.4",
    "bytes": "^3.1.2",
    "classnames": "^2.3.1",
    "connect-timeout": "^1.9.1",
    "core-js": "^3.25.0",
    "cross-fetch": "^3.1.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "http-proxy-middleware": "^2.0.9",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-http-backend": "^3.0.2",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "moment-range": "^4.0.2",
    "node-fetch": "^3.2.10",
    "print-js": "^1.6.0",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-helmet": "^6.1.0",
    "react-i18next": "11.18.4",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.15.1",
    "react-pdf": "^9.2.1",
    "react-redux": "^8.0.2",
    "react-resize-detector": "^7.1.2",
    "react-router-dom": "^7.5.2",
    "react-select": "^5.4.0",
    "react-to-print": "^2.14.7",
    "redux": "^4.2.0",
    "sprintf-js": "^1.1.2",
    "styled-components": "^5.3.7",
    "url": "^0.11.4",
    "uuid": "^8.3.2",
    "vite": "^6.3.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.3",
    "@babel/runtime": "^7.18.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bytes": "^3.1.5",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.14.184",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^10.0.6",
    "@types/mustache": "^4.2.1",
    "@types/react": "^18.0.17",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.11",
    "@types/react-loadable": "^5.5.6",
    "@types/react-modal": "^3.13.1",
    "@types/react-redux": "^7.1.24",
    "@types/react-resize-detector": "^5.0.0",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "babel-plugin-styled-components": "^2.0.7",
    "check-peer-dependencies": "^4.1.0",
    "cross-env": "^7.0.3",
    "es6-promise": "^4.2.8",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "fast-xml-parser": "^4.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "mustache": "^4.2.0",
    "prop-types": "^15.8.1",
    "redux-mock-store": "^1.5.4",
    "standardx": "^7.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "babel-jest": "26.6.0"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "rules": {
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    },
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "fragment": "Fragment",
        "version": "detect",
        "flowVersion": "0.53"
      },
      "propWrapperFunctions": [
        "forbidExtraProps",
        {
          "property": "freeze",
          "object": "Object"
        },
        {
          "property": "myFavoriteWrapper"
        }
      ],
      "linkComponents": [
        "Hyperlink",
        {
          "name": "Link",
          "linkAttribute": "to"
        }
      ]
    }
  },
  "standardx": {
    "ignore": [
      "public/",
      "*.snap"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "react-hooks"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "globals": [
      "act",
      "afterAll",
      "afterEach",
      "beforeAll",
      "beforeEach",
      "Blob",
      "describe",
      "Element",
      "Event",
      "expect",
      "fetch",
      "CustomEvent",
      "File",
      "FileReader",
      "HTMLAnchorElement",
      "HTMLButtonElement",
      "HTMLCanvasElement",
      "HTMLInputElement",
      "HTMLTextAreaElement",
      "HTMLSelectElement",
      "HTMLElement",
      "it",
      "jest",
      "JSX",
      "MessageEvent",
      "mount",
      "React",
      "ReactDOM",
      "render",
      "renderer",
      "shallow",
      "Window",
      "XMLHttpRequest"
    ]
  },
  "type": "module"
}
