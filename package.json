  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "standard": {
    "ignore": [
      "public/*.js"
    ],
    "parser": "babel-eslint",
    "globals": [
      "Blob",
      "File",
      "XMLHttpRequest",
      "HTMLCanvasElement",
      "FileReader",
      "ReactDOM",
      "fetch",
      "describe",
      "it",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "shallow",
      "mount",
      "render",
      "renderer",
      "jest"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      },
      "src/actions": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverageFrom": [
      "src/actions/*.js",
      "src/applications/*.js",
      "src/applications/**/*.js",
      "src/components/*.js",
      "src/components/**/*.js",
      "src/pages/*.js",
      "src/pages/**/*.js",
      "src/reducers/*.js",
      "src/utils/*.js",
      "src/widgets/*.js",
      "src/widgets/**/*.js"
    ]
  }
