{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  },
  "tags": [
    "-lintignore"
  ],
  "entry": ["src/index.tsx", "scripts/*.js", "server.mjs"],
  "project": ["src/**", "scripts/**"],
  "ignore": ["**/*.test.ts", "**/*.test.tsx", "**/*.test.js"],
  "vite": {
    "config": [
      "vite.config.{js,mjs,ts,cjs,mts,cts}"
    ]
  }
}
