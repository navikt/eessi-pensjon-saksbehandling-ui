{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "process": true,
    "Promise" : true,
    "module": true,
    "require": true,
    "__dirname": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-console": 0,
    "indent": ["error", 2],
    "quotes": [2, "single"]
    // overrides
  }
}
